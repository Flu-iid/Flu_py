  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python_launch",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "py_x",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "externalTerminal",
      "justMyCode": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "JS_Launch",
      "program": "${file}",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    }
  ]
}
