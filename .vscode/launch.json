{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python_launch",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "py_x",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "externalTerminal",
      "justMyCode": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "JS_Launch",
      "program": "${file}",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/arsha/Project/py-algo",
      "program": "c:/Users/arsha/Project/py-algo/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}